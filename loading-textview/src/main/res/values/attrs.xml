<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingTextView">
        <attr name="start_color" format="color"/>
        <attr name="end_color" format="color"/>
        <attr name="main_color" format="color"/>
        <attr name="loading_style" format="enum">
            <enum name="normal" value="0"/>
            <enum name="crazy" value="1"/>
        </attr>
        <attr name="loading_gradient" format="boolean"/>
    </declare-styleable>
</resources>